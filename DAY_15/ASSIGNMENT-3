//Write a c program to print x to the power of y value
#include <stdio.h>
int main() {
    int x, y;
    long double result = 1.0;
    printf("Enter x value: ");
    scanf("%d", &x);
    printf("Enter y value: ");
    scanf("%d", &y);

    while (y != 0) {
        result *= x;
        --y;
    }
    printf("your Answer = %.0Lf", result);
    return 0;
}

